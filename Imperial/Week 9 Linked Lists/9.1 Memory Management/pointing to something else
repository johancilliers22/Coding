#include <iostream>
 
int main(){
    int a, b;
    int* pi; 
 
    a = 2;
    b = 3;
    pi = &a;
 
    std::cout << a << std::endl;  // 2
    std::cout << b << std::endl;  // 3 
    std::cout << *pi << std::endl;// 2
 
    pi = &b;
 
    std::cout << a << std::endl;  // 2
    std::cout << b << std::endl;  // 3 
    std::cout << *pi << std::endl;// 3
 
}