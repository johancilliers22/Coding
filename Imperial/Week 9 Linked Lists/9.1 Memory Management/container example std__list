#include <iostream>
#include <list>
 
int main(){
 
    std::list<int> numbers;
 
    numbers.push_back(1);
    numbers.push_back(2);
    // adds elements to the data structure
 
    // so far it doesn't seem any different from std::vector
    // however we can't e.g. do this:
    // std::cout << numbers[0] << std::endl;
 
}