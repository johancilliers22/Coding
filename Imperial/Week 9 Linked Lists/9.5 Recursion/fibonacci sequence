#include <iostream>
#include <fstream>
#include <cstdlib>
#include <vector>

int fib(int n){
    if(n == 0){
        return 0; // base case 
    }
    if(n==1){
      return 1;
    }
    else{
        return fib(n-1) + fib(n-2); // recursive call
    }
}

int main(){
    std::cout<<fib(4)<<std::endl;
}