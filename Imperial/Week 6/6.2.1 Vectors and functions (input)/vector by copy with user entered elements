#include <iostream>
#include <vector>
 
// maybe not the most efficient way:
double sum_vector(std::vector<double> vin){
 
    double sum = 0;
 
    for(int i = 0; i < vin.size(); i++){
        sum = sum + vin[i];
    }
 
    return sum;
 
}
 
int main(){
    std::vector<double> v;
    int n,i,tmp;
    std::cout << "How many vectors do you want to sum? " << std::endl;
    std::cin >> n;
    std::cout << "Enter vectors: " << std::endl;
    for(int i = n; i > 0; i--){
        std::cin >> tmp;
        v.push_back(tmp);
    }
    std::cout << "the sum is: " << sum_vector(v) << std::endl;
 
}