#include <iostream>
#include <vector>
 
int main(){
    std::vector<int> v; 
    int n,m,b;
    std::cout << "Please enter 3 numbers: " << std::endl;
    std::cin >> n >> m >> b;
    v.push_back(n);
    v.push_back(m);
    v.push_back(b);
    std::cout << "Result: " << v[0] * (v[1] + v[2]) << std::endl;
}