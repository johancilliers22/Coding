#include <iostream>
#include <vector>
 
int main(){
    std::vector<int> sequence;
    int i,n,tmp;
    std::cout << "please enter a number n that is positive" << std::endl;
    std::cin >> n;
    for(int i = n; i > 0; i--){
        std::cout << "Please enter your sequence of numbers: " << std::endl;
        std::cin >> tmp;
        sequence.push_back(tmp);
    }
    std::cout << "Please enter upper limit filter value: " << std::endl;
    int t;
    std::cin >> t;
    while (n-1 > -1){
      if (sequence[n-1] < t){
        std::cout << sequence[n-1];
      }
      n--;
    }
}