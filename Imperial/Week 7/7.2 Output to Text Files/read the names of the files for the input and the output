#include <iostream>
#include <fstream>
#include <cstdlib>
#include <vector>
#include <string> 
 
int main(){
 
    std::string inputfilename;
    // as usual not a special name
    std::cout << "enter name of input file" << std::endl;
 
    std::cin >> inputfilename;
 
    std::ifstream infile; 
    infile.open(inputfilename);
    std::vector<int> v;
    if(!infile.is_open()){ 
        std::cout << "error opening file" << std::endl;
        return EXIT_FAILURE;
    }
    int tmp;
    while(infile >> tmp){
        v.push_back(tmp); 
    }
    infile.close();
    std::string outputfilename;
    std::cout << "enter name of the output file" << std::endl;
    std::cin >> outputfilename;
    std::ofstream outfile;
    outfile.open(outputfilename);
    int n = v.size();
    int i;
    if(!outfile.is_open()){ 
        std::cout << "error opening file" << std::endl;
        return EXIT_FAILURE;
    }
    for(i=0;n>i;i++){
      outfile << v[i] << std::endl;
    }
    outfile.close();
}